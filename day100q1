#include <stdio.h>

struct Student {
    int id;
    float marks;
};

int main() {
    struct Student s1;
    struct Student *ptr;

    ptr = &s1;

    ptr->id = 101;
    ptr->marks = 92.5;

    printf("Student Details:\n");
    printf("ID = %d\n", ptr->id);
    printf("Marks = %.2f\n", ptr->marks);

    return 0;
}
