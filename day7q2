#include <stdio.h>

int main() {
    char ch;
    printf("Enter the character: ");
    scanf("%c", &ch);

    if (ch == 'A' || ch == 'a' || ch == 'E' || ch == 'e' || 
        ch == 'I' || ch == 'i' || ch == 'O' || ch == 'o' || 
        ch == 'U' || ch == 'u') {
        printf("The character is a vowel\n");
    }
    else {
        printf("The character is a consonant\n");
    }

    return 0;
}
