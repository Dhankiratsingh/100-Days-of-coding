#include <stdio.h>
#include <string.h>

int main() {
    char s[100], t[100];
    int freqS[26] = {0}, freqT[26] = {0};

    printf("Enter first string (s): ");
    scanf("%s", s);
    printf("Enter second string (t): ");
    scanf("%s", t);

    // If lengths differ, they can't be anagrams
    if (strlen(s) != strlen(t)) {
        printf("Not Anagram");
        return 0;
    }

    // Count character frequencies for both strings
    for (int i = 0; s[i] != '\0'; i++) {
        freqS[s[i] - 'a']++;
        freqT[t[i] - 'a']++;
    }

    // Compare frequency arrays
    for (int i = 0; i < 26; i++) {
        if (freqS[i] != freqT[i]) {
            printf("Not Anagram");
            return 0;
        }
    }

    printf("Anagram");
    return 0;
}
