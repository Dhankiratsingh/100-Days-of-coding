#include <stdio.h>

int main() {
    int n, x;
    printf("Enter number of elements: ");
    scanf("%d", &n);

    int arr[n];
    printf("Enter %d sorted elements: ", n);
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
    }

    printf("Enter x: ");
    scanf("%d", &x);

    int left = 0, right = n - 1;
    int ans = -1;

    // Binary search for ceil
    while (left <= right) {
        int mid = (left + right) / 2;

        if (arr[mid] >= x) {
            ans = mid;        // possible ceil found
            right = mid - 1;  // look for smaller one on left
        } else {
            left = mid + 1;   // move right
        }
    }

    printf("%d\n", ans);
    return 0;
}
