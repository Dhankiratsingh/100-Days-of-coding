#include <stdio.h>

int main() {
    int num1,num2,num3;
    printf("Enter three numbers :");
    scanf("%d\n%d\n%d",&num1,&num2,&num3);
    if(num1>num2){
         if(num1>num3){
            printf("Number 1 is greatest");
        }
        else{
            printf("Number 3 is greatest");
    }
  }  else{
      if(num2>num3){
          printf("Number 2 is  greatest");
      }
      else{
          printf("Number 3 is greatest");
      }
      }
      return 0;
  }
