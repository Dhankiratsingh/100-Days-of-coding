#include <stdio.h>
#include <ctype.h>

int main() {
    FILE *fin = fopen("input.txt", "r");
    FILE *fout = fopen("output.txt", "w");
    if (!fin || !fout) {
        printf("Error opening file.\n");
        return 1;
    }

    char ch;
    while ((ch = fgetc(fin)) != EOF) {
        if (islower(ch))
            ch = toupper(ch);
        fputc(ch, fout);
    }

    fclose(fin);
    fclose(fout);
    printf("Conversion complete. Check output.txt for results.\n");
    return 0;
}
